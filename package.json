{
    "name": "cursor-patch",
    "displayName": "Cursor Patch",
    "publisher": "cursor-patch",
    "description": "A VS Code extension to patch Cursor editor",
    "version": "0.0.4",
    "engines": {
        "vscode": "^1.93.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./extension.js",
    "contributes": {
        "configuration": {
            "title": "Cursor Patch",
            "properties": {
                "cursorPatch.storagePath": {
                    "type": "string",
                    "default": "",
                    "description": "自定义 storage.json 文件的路径。留空则使用默认路径。"
                },
                "cursorPatch.customMachineId": {
                    "type": "string",
                    "default": "",
                    "description": "自定义 machineId。留空则随机生成。"
                }
            }
        },
        "commands": [
            {
                "command": "cursor-patch.patch",
                "title": "Cursor: Patch Machine ID",
                "category": "Cursor"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js",
        "package": "vsce package --baseContentUrl https://raw.githubusercontent.com/chengazhen/cursor-patch/main",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "@types/vscode": "^1.93.0",
        "@types/node": "20.x",
        "eslint": "^8.56.0",
        "@vscode/test-electron": "^2.3.8",
        "@vscode/vsce": "^2.24.0"
    },
    "icon": "icon.png",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/chengazhen/cursor-patch.git"
    }
}
